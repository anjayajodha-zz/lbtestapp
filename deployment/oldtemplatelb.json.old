            "name": "[parameters('loadBalancers_testloadbalancer_name')]",
            "apiVersion": "2016-03-30",
            "location": "westus",
            "properties": {
                "frontendIPConfigurations": [
                    {
                        "name": "LoadBalancerFrontEnd",
                        "properties": {
                            "privateIPAllocationMethod": "Dynamic",
                            "publicIPAddress": {
                                "id": "[resourceId('Microsoft.Network/publicIPAddresses', parameters('publicIPAddresses_testpublicip_name'))]"
                            }
                        }
                    }
                ],
                "backendAddressPools": [
                    {
                        "name": "backpool"
                    }
                ],
                "loadBalancingRules": [
                    {
                        "name": "lbrule",
                        "properties": {
                            "frontendIPConfiguration": {
                                "id": "[parameters('loadBalancers_testloadbalancer_id')]"
                            },
                            "frontendPort": 80,
                            "backendPort": 8081,
                            "enableFloatingIP": false,
                            "idleTimeoutInMinutes": 4,
                            "protocol": "Tcp",
                            "loadDistribution": "Default",
                            "backendAddressPool": {
                                "id": "[parameters('loadBalancers_testloadbalancer_id_1')]"
                            },
                            "probe": {
                                "id": "[parameters('loadBalancers_testloadbalancer_id_2')]"
                            }
                        }
                    }
                ],
                "probes": [
                    {
                        "name": "testapp",
                        "properties": {
                            "protocol": "Http",
                            "port": 8081,
                            "requestPath": "/",
                            "intervalInSeconds": 5,
                            "numberOfProbes": 2
                        }
                    }
