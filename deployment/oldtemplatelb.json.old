                        "name": "[parameters('virtualMachines_webvm1_name')]",
                        "createOption": "FromImage",
                        "vhd": {
                            "uri": "[concat('https://722088westus.blob.core.windows.net/vhds/', parameters('virtualMachines_webvm1_name'),'2016819143722.vhd')]"
